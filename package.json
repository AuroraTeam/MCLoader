        "@types/progress-stream": "^2.0.0",
        "@types/rimraf": "^3.0.0",
        "@types/yauzl": "^2.9.1",
        "@typescript-eslint/eslint-plugin": "^4.15.1",
        "@typescript-eslint/parser": "^4.15.1",
        "colors": "^1.4.0",
        "eslint": "^7.20.0",
        "import-sort-style-module": "^6.0.0",
        "p-map": "^4.0.0",
        "prettier": "^2.0.5",
